                <strong>Masters Degree:</strong> @ViewBag.Masters <br />
                <strong>Bachelors Degree:</strong> @ViewBag.Bachelors <br />
            </div>
            <div class="row">
                <div class="col-md-4">
                    <h3><strong>Coordinator</strong>  </h3> 
                    <br/>
                    <img src="@Html.Raw(ViewBag.Picture)" />
                </div>
            </div>
            </div>
        </div>

