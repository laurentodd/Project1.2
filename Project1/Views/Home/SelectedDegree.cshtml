
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <ol class="breadcrumb">
        <li class="active"><a href='@Url.Action("Index", "Home")'>Home</a></li>
        <li><a href='@Url.Action("Degrees", "Home")'>Degrees</a></li>
    </ol>
</div>  

<h2><strong>@ViewBag.Title</strong> </h2>

<p> 
    <div class="row">
        <div class="col-md-4">
            <h3><strong>About</strong>  </h3> <br />
           <strong>Degree:</strong>  @ViewBag.DegreeName 
            <strong>Degree Coordinator:</strong> @ViewBag.Coordinator <br />
            <strong>Professor Title: </strong> @ViewBag.ProfTitle <br />
            <strong>Professor Office Address: </strong> @ViewBag.Address <br />
            <br />
        </div>
        <div class="row">
            <div class="col-md-4">
